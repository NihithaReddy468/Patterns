Pattern 1

n=int(input())
for i in range(1,n+1):
    for j in range(1,n+1):
        print("*",end="")
    print()
    
    
    
Pattern II

n=int(input())
for i in range(0,n):
    for j in range(1,n+1):
        print(i+1,end="")
    print()
    
    
    
Pattern III

n=int(input())
for i in range(1,n+1):
    for j in range(1,n+1):
        if(i>=j):
            print("*",end="")
    print()
    
    
    
Pattern IV

n=int(input())
for i in range(1,n+1):
    for j in range(1,n+1):
        if(i<=j):
            print("*",end="")
    print()
    
    
